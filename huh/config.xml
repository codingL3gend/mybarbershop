    <vs:feature>org.apache.cordova.geolocation@0.3.8</vs:feature>
    <vs:feature>org.apache.cordova.media@0.2.11</vs:feature>
    <vs:feature>org.apache.cordova.media-capture@0.3.1</vs:feature>
    <vs:feature>com.msopentech.websql@0.0.4</vs:feature>
    <vs:feature>org.apache.cordova.device-orientation@0.3.7</vs:feature>
    <vs:feature>net.yoik.cordova.plugins.screenorientation</vs:feature>
  </vs:features>
  <gap:plugin name="net.yoik.cordova.plugins.screenorientation"/>
  <vs:platformSpecificValues />
